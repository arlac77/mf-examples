{
  "name": "mf-examples",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "packageManager": "npm@11.10.0",
  "description": "examples entry page",
  "keywords": [
    "example",
    "vite",
    "web"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "0BSD",
  "scripts": {
    "prepare": "node --run prepare:vite",
    "prepare:vite": "vite build",
    "start": "vite",
    "test": "true",
    "lint": "node --run lint:css",
    "lint:css": "stylelint ./src**/*.css",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/github": "^12.0.6",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "mf-hosting-frontend": "^4.0.2",
    "mf-styling": "^3.3.9",
    "npm-pkgbuild": "^20.6.4",
    "semantic-release": "^25.0.3",
    "stylelint": "^17.3.0",
    "stylelint-config-standard": "^40.0.0",
    "vite": "^7.3.1",
    "vite-plugin-compression2": "^2.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arlac77/mf-examples.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/mf-examples/issues"
  },
  "homepage": "https://github.com/arlac77/mf-examples#readme",
  "pkgbuild": {
    "content": {
      "${install_dir}": "build/"
    },
    "dependencies": {
      "mf-styling": ">=3.3.7",
      "svelte-command": ">=3.0.56",
      "svelte-common": ">=6.19.69",
      "svelte-entitlement": ">=2.0.65",
      "svelte-guard-history-router": ">=7.0.14",
      "svelte-log-view": ">=7.0.6",
      "svelte-session-manager": ">=3.0.80",
      "svelte-websocket-store": ">=1.2.6",
      "svelte-repository-provider": ">=2.0.76",
      "kronos-svelte-components": ">=2.4.94",
      "resume-renderer-example": ">=1.0.2",
      "npm-navigator": ">=2.2.1"
    },
    "example": true,
    "frontend": true,
    "http_path": "${http_base_path}",
    "install_dir": "${install_base_dir}"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      [
        "@semantic-release/exec",
        {
          "publishCmd": "npx npm-pkgbuild --publish dist"
        }
      ],
      [
        "@semantic-release/github",
        {
          "assets": [
            {
              "path": "dist/*.deb",
              "label": "any Debian Package"
            },
            {
              "path": "dist/*any.pkg.*",
              "label": "any Arch Linux Package"
            },
            {
              "path": "dist/*x86_64.pkg.*",
              "label": "x86_64 Arch Linux Package"
            },
            {
              "path": "dist/*aarch64.pkg.*",
              "label": "arm64 Arch Linux Package"
            },
            {
              "path": "dist/*noarch.rpm",
              "label": "noarch RPM"
            },
            {
              "path": "dist/*x86_64.rpm",
              "label": "x86_64 RPM"
            },
            {
              "path": "dist/*aarch64.rpm",
              "label": "arm64 RPM"
            }
          ]
        }
      ]
    ]
  },
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-npm-pkgbuild",
      "arlac77/template-vite",
      "arlac77/template-web-app"
    ]
  }
}
